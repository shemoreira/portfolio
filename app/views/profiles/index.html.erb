<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Github</th>
     <th>Bio</th>
      <th colspan="3"></th>
    </tr>

    </thead>

  <tbody>
    <th><%= @profile.name  %></th>
    <th><%= @profile.email  %></th>
    <th><%= @profile.github  %></th>
    <th><%= @profile.bio  %></th>


      <% @profile.skills.each do |skill| %>
        <th><%= skill.name  %></th>
        <th><%= skill.years  %></th>
      <% end %>
  </tbody>
</table>
<%= link_to 'New Profile', new_profile_path %>
